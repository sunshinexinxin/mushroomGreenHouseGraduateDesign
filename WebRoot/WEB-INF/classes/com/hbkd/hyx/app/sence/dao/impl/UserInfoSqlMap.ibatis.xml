<?xml version="1.0" encoding="UTF-8"?>    
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"    
     "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="UserInfoMapper">
	<resultMap id="userInfoMapList" type="UserInfo">
		<!-- 个人家庭信息 -->
		<result column="id" property="id" />
		<result column="userId" property="userId" />
		<result column="relationShip" property="relationShip" />
		<result column="name" property="name" />
		<result column="birthday" property="birthday" />
		<result column="tel" property="tel" />
		<result column="email" property="email" />
		<result column="position" property="position" />
		<!-- 个人职业变动信息 -->
		<result column="occTime" property="occTime" />
		<result column="office" property="office" />
		<result column="leader" property="leader" />
	</resultMap>
	<select id="getUserInfoFamiList" resultMap="userInfoMapList">
		select * from
		TB_ML_USERS_FAMI WHERE userId = #{id,jdbcType=NVARCHAR}
	</select>

	<!--个人职业变动信息-->
	<select id="getUserOccList" resultMap="userInfoMapList">
		select * from
		TB_ML_USERS_OCC WHERE userId = #{id,jdbcType=NVARCHAR}
	</select>


</mapper>